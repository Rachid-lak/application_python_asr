       stage('SCM') {
         steps {
            script {   
              checkout scm
             }
          }
       }
         stage('SonarQube Analysis') {
            steps {
                script {
                    def scannerHome = tool 'sonar-runner'
                    withSonarQubeEnv() {
                        sh "${scannerHome}/sonar-runner \
  -Dsonar.projectKey=python \
  -Dsonar.sources=. \
  -Dsonar.host.url=http://127.0.0.1:9000/"
                    }
                }
            }
        }

   }
    
}
