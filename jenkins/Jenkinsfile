pipeline{ 
    environment {
        registry = 'rachidias/application_python_asr'
        registryCredential = 'Dockerhub'
        dockerImage = ''
    }
  
   agent any
    stages {
   

    stage('sonarqube'){

         node {
    stage('SCM') {
    checkout scm
          }
    stage('SonarQube Analysis') {
       def scannerHome = tool 'SonarQubeScanner';
       withSonarQubeEnv() {
      sh "${scannerHome}/bin/sonar-scanner"
          }
        }
     }

  }
 }
    

